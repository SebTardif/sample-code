curl -k \
  -d "{'username':'admin', 'password':$TL_ADMIN_PW}" \
  -H 'Content-Type: application/json' \
  -X POST \
  -d "{'key': $TL_LICENSE}" \
  $TL_CONSOLE_HTTPS/api/v1/settings/license

status=$?

if [ $status -eq 0 ]
then
  echo "Successfully installed license"
else
  echo "License install failed, $status"
fi


